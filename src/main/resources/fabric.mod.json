{
  "schemaVersion": 1,
  "id": "customworldicons",
  "version": "${version}",

  "name": "Custom World Icons",
  "description": "Allows you to easily add custom icons to your world upon creation.",
  "authors": [
    "Iamaprogrammer"
  ],
  "contact": {
    "homepage": "https://github.com/lamaprogramer/CustomWorldIcons",
    "sources": "https://github.com/lamaprogramer/CustomWorldIcons"
  },

  "license": "MIT",
  "icon": "assets/customworldicons/icon.png",

  "environment": "*",
  "entrypoints": {
